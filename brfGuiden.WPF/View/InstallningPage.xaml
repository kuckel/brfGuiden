<Page x:Class="brfGuiden.WPF.View.InstallningPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      xmlns:viewmodel="clr-namespace:brfGuiden.WPF.ViewModel"
      Title="DashboardPage"
      d:DataContext="{d:DesignInstance local:DashboardPage, IsDesignTimeCreatable=False}"  
      xmlns:local="clr-namespace:brfGuiden.WPF.View"
      ui:Design.Background="{DynamicResource ApplicationBackgroundBrush}"
      ui:Design.Foreground="{DynamicResource TextFillColorPrimaryBrush}" 
      Foreground="{DynamicResource TextFillColorPrimaryBrush}" Margin="0"     
      mc:Ignorable="d"  d:DesignHeight="450" d:DesignWidth="800" >

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="*" />            
        </Grid.RowDefinitions> 
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            
        </Grid.ColumnDefinitions>




        <TextBlock Text="Inställningar" Style="{StaticResource BlueTextblockStyle}" HorizontalAlignment="Stretch" 
          Width="auto" VerticalAlignment="Center"   Grid.Row="0"  Height="30" Padding="10 5 5 5"  />


    </Grid>
</Page>
